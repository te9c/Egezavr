<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Egezavr.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Egezavr"
    >

    <Tab>
        <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:WeekPage}"
        Route="MainPage" />
        <ShellContent
            Title="SecPage"
            ContentTemplate="{DataTemplate local:TaskPage}"
            Route="SecPage"/>
    </Tab>

</Shell>
